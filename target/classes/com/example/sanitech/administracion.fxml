<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sanitech.AdministracionController">
   <children>
      <TableView fx:id="tbUsuarios" layoutX="286.0" layoutY="33.0" prefHeight="500.0" prefWidth="1000.0">
        <columns>
          <TableColumn fx:id="colId" prefWidth="333.0" text="ID" />
          <TableColumn fx:id="colNombre" prefWidth="333.0" text="Nombre" />
            <TableColumn fx:id="colRol" prefWidth="334.0" text="Rol" />
        </columns>
      </TableView>
      <Button fx:id="btnCrear" layoutX="83.0" layoutY="33.0" mnemonicParsing="false" text="Crear usuario" />
      <Button fx:id="btnEliminar" layoutX="90.0" layoutY="507.0" mnemonicParsing="false" onAction="#eliminarUsuario" text="Eliminar usuario" />
   </children>
</AnchorPane>
